<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.heima.chickenhousebackend.mapper.VideoMapper">  <!--位置-->
    <!--根据用户id获取用户视频展示的数据-->
    <select id="getAllVideoListById" resultType="com.heima.chickenhousebackend.pojo.Video">
        select *
        from video
        where user_id = #{id}
    </select>
</mapper>